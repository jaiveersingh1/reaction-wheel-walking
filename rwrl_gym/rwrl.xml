<mujoco model="rwrl">
	<compiler angle="degree" coordinate="global" inertiafromgeom="true"/>
	<default>
		<joint armature=".1" damping=".01" limited="true" solimplimit="0 .8 .03" solreflimit=".02 1" stiffness="8"/>
		<geom conaffinity="0" condim="3" contype="1" friction="0.8 .1 .1" rgba="0.8 0.6 .4 1" solimp="0.0 0.8 0.01" solref="0.02 1"/>
		<motor ctrllimited="true" ctrlrange="-1 1"/>
	</default>
	<option gravity="0 0 -9.81" timestep="0.01"/>
	<worldbody>
		<body name="torso">
			<joint armature="0" axis="1 0 0" damping="0" limited="false" name="ignorex" pos="0 0 0" stiffness="0" type="slide"/>
			<joint armature="0" axis="0 0 1" damping="0" limited="false" name="ignorez" pos="0 0 0" stiffness="0" type="slide"/>
			<joint armature="0" axis="0 1 0" damping="0" limited="false" name="ignorey" pos="0 0 0" stiffness="0" type="hinge"/>
			<geom type="box" pos="0 0 .1875" size=".0375 .075 .0375" rgba="1 1 1 .5"/>
			<!-- <inertial pos="0 0 .0375" mass=".05" diaginertia="1e-6 1e-6 1e-6"/> -->
			<inertial mass=".5"/>
			<body name="x_wheel">
				<geom type="cylinder" fromto=".005 -.04 .1875 -.005 -.04 .1875" size=".03" rgba="0 1 0 1"/>
				<!-- <inertial pos="0 -.04 .0375" mass=".1" diaginertia="1e-5 1e-5 1e-5"/> -->
				<inertial mass=".2"/>
				<joint name="x_joint" type="hinge" pos="0 -.04 .1875" axis="1 0 0" limited="false"/>
			</body>

			<body name="y_wheel">
				<geom type="cylinder" fromto="0 .065 .1875 0 .055 .1875" size=".03" rgba="1 0 0 1"/>
				<!-- <inertial pos="0 .06 .0375" mass=".1" diaginertia="1e-5 1e-5 1e-5"/> -->
				<inertial mass=".2"/>
				<joint name="y_joint" type="hinge" pos="0 .06 .1875" axis="0 1 0" limited="false"/>
			</body>

			<body name="z_wheel">
				<geom type="cylinder" fromto="0 .0225 .1925 0 .0225 .1825" size=".03" rgba="0 0 1 1"/>
				<!-- <inertial pos="0 .0225 .0375" mass=".1" diaginertia="1e-5 1e-5 1e-5"/> -->
				<inertial mass=".2"/>
				<joint name="z_joint" type="hinge" pos="0 .0225 .18755" axis="0 0 1" limited="false"/>
			</body>

			<body name="left_leg">
				<geom type="cylinder" fromto="0 -.0375 .15 0 -.0375 .0375" size=".03" rgba="1 1 1 1"/>
				<!-- <inertial pos="0 -.0375 -.05625" mass=".05" diaginertia="1e-6 1e-6 1e-6"/> -->
				<inertial mass=".4"/>
				<joint name="left_joint" type="hinge" pos="0 -.0375 .15" axis="0 -1 0" range="-60 60" damping="5"/>

				<geom type="sphere" pos="0 -.0375 .0375" size=".0375 .0375 .0375" rgba="1 1 1 1"/>
				<!-- <inertial pos="0 -.0375 -.1125" mass=".01" diaginertia="1e-6 1e-6 1e-6"/> -->
				<inertial mass=".1"/>
			</body>

			<body name="right_leg">
				<geom type="cylinder" fromto="0 .0375 .15 0 .0375 .0375" size=".03" rgba="1 1 1 1"/>
				<!-- <inertial pos="0 .0375 -.05625" mass=".05" diaginertia="1e-6 1e-6 1e-6"/> -->
				<inertial mass=".4"/>
				<joint name="right_joint" type="hinge" pos="0 .0375 .15" axis="0 -1 0" range="-60 60" damping="5"/>

				<geom type="sphere" pos="0 .0375 .0375" size=".0375 .0375 .0375" rgba="1 1 1 1"/>
				<inertial mass=".1"/>
				<!-- <inertial pos="0 .0375 -.1125" mass=".01" diaginertia="1e-6 1e-6 1e-6"/> -->
			</body>

		</body>
	</worldbody>
	<actuator>
		<motor ctrllimited="true" gear="200.0" joint="x_joint" name="x_joint"/>
		<motor ctrllimited="true" gear="200.0" joint="y_joint" name="y_joint"/>
		<motor ctrllimited="true" gear="200.0" joint="z_joint" name="z_joint"/>
		<motor ctrllimited="true" gear="200" joint="left_joint" name="left_joint"/>
		<motor ctrllimited="true" gear="200" joint="right_joint" name="right_joint"/>
	</actuator>
</mujoco>