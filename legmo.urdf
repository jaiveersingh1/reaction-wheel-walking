<?xml version="1.0"?> 
<robot name="legmo">

    <!-- Colors -->
    <material name="translucent">
        <color rgba="1.0 1.0 1.0 0.25"/>
    </material>
    <material name="red">
        <color rgba="1.0 0.0 0.0 1.0"/>
    </material>
    <material name="green">
        <color rgba="0.0 1.0 0.0 1.0"/>
    </material>
    <material name="blue">
        <color rgba="0.0 0.0 1.0 1.0"/>
    </material>

    <!-- Base link of robot -->
    <link name="base_link">
		<visual>
			<geometry>
                <!-- TODO: Update Z once Matt finishes CAD -->
				<box size="0.055 0.115 0.070"/>
			</geometry>
			<material name="translucent"/>

            <!-- xyz = 0, 0, z/2-->
            <origin rpy="0 0 0" xyz="0 0 0.035"/>
		</visual>
		<collision>
			<geometry>
                <!-- Same as visual-->
				<box size="0.055 0.115 0.070"/>
			</geometry>
 
            <!-- Same as visual-->
            <origin rpy="0 0 0" xyz="0 0 0.025"/>
		</collision>
		<inertial>
            <!-- Estimate from Fusion360-->
			<mass value="0.500"/>
 
            <!-- ixx = 1/12 * m * (y^2 + z^2)-->
            <!-- iyy = 1/12 * m * (x^2 + z^2)-->
            <!-- izz = 1/12 * m * (x^2 + y^2)-->
			<inertia ixx="7.55e-4" ixy="0" ixz="0" iyy="3.30e-4" iyz="0" izz="6.77e-4"/>
		</inertial>
	</link>

	<!-- Right leg of robot -->
    <link name="right_leg">
		<visual>
			<geometry>
				<box size="0.060 0.052 0.115"/>
			</geometry>
			<material name="red"/>

            <!-- xyz = 0, -y/2, 84mm - z-->
            <origin rpy="0 0 0" xyz="0 -0.026 -0.0265"/>
		</visual>
		<collision>
			<geometry>
                <!-- Same as visual-->
				<box size="0.060 0.052 0.115"/>
			</geometry>

            <!-- Same as visual-->
            <origin rpy="0 0 0" xyz="0 -0.026 -0.0265"/>
		</collision>
		<inertial>
            <!-- Estimate from Fusion360-->
			<mass value="0.300"/>

            <!-- ixx = 1/12 * m * (y^2 + z^2)-->
            <!-- iyy = 1/12 * m * (x^2 + z^2)-->
            <!-- izz = 1/12 * m * (x^2 + y^2)-->
			<inertia ixx="3.98e-4" ixy="0" ixz="0" iyy="6.61e-4" iyz="0" izz="1.58e-4"/>
		</inertial>
	</link>

    <!-- Left leg of robot -->
    <link name="left_leg">
		<visual>
			<geometry>
				<box size="0.060 0.052 0.115"/>
			</geometry>
			<material name="blue"/>

            <!-- xyz = 0, y/2, 84mm - z-->
            <origin rpy="0 0 0" xyz="0 0.026 -0.0265"/>
		</visual>
		<collision>
			<geometry>
                <!-- Same as visual-->
				<box size="0.060 0.052 0.115"/>
			</geometry>

            <!-- Same as visual-->
            <origin rpy="0 0 0" xyz="0 0.026 -0.0265"/>
		</collision>
		<inertial>
            <!-- Estimate from Fusion360-->
			<mass value="0.300"/>

            <!-- ixx = 1/12 * m * (y^2 + z^2)-->
            <!-- iyy = 1/12 * m * (x^2 + z^2)-->
            <!-- izz = 1/12 * m * (x^2 + y^2)-->
			<inertia ixx="3.98e-4" ixy="0" ixz="0" iyy="6.61e-4" iyz="0" izz="1.58e-4"/>
		</inertial>
	</link>

    <!-- Roll reaction wheel -->
	<link name="roll_rw">
		<visual>
			<geometry>
                <!-- TODO: Update from Matt-->
				<cylinder length="0.01" radius="0.025"/>
			</geometry>
			<material name="red"/>
		</visual>
		<collision>
			<geometry>
                <!-- Same as visual-->
				<cylinder length="0.01" radius="0.025"/>
			</geometry>
		</collision>
		<inertial>
			<mass value="0.200"/>
            <!-- ixx = 1/12 * m * (3 * r^2 + h^2)-->
            <!-- iyy = 1/12 * m * (3 * r^2 + h^2)-->
            <!-- izz = 1/12 * m * r^2 -->
			<inertia ixx="3.29e-5" ixy="0" ixz="0" iyy="3.29e-5" iyz="0" izz="1.04e-5"/>
		</inertial>
	</link>
    
    <!-- Pitch reaction wheel -->
	<link name="pitch_rw">
		<visual>
			<geometry>
                <!-- TODO: Update from Matt-->
				<cylinder length="0.01" radius="0.025"/>
			</geometry>
			<material name="blue"/>
		</visual>
		<collision>
			<geometry>
                <!-- Same as visual-->
				<cylinder length="0.01" radius="0.025"/>
			</geometry>
		</collision>
		<inertial>
			<mass value="0.200"/>
            <!-- ixx = 1/12 * m * (3 * r^2 + h^2)-->
            <!-- iyy = 1/12 * m * (3 * r^2 + h^2)-->
            <!-- izz = 1/12 * m * r^2 -->
			<inertia ixx="3.29e-5" ixy="0" ixz="0" iyy="3.29e-5" iyz="0" izz="1.04e-5"/>
		</inertial>
	</link>
    
    <!-- Yaw reaction wheel -->
    <link name="yaw_rw">
		<visual>
			<geometry>
                <!-- TODO: Update from Matt-->
				<cylinder length="0.01" radius="0.025"/>
			</geometry>
			<material name="green"/>
		</visual>
		<collision>
			<geometry>
                <!-- Same as visual-->
				<cylinder length="0.01" radius="0.025"/>
			</geometry>
		</collision>
		<inertial>
			<mass value="0.200"/>
            <!-- ixx = 1/12 * m * (3 * r^2 + h^2)-->
            <!-- iyy = 1/12 * m * (3 * r^2 + h^2)-->
            <!-- izz = 1/12 * m * r^2 -->
			<inertia ixx="3.29e-5" ixy="0" ixz="0" iyy="3.29e-5" iyz="0" izz="1.04e-5"/>
		</inertial>
	</link>

    <joint name="base_to_right_leg" type="revolute">
		<parent link="base_link"/>
		<child link="right_leg"/>
		<axis xyz="0 -1 0"/>

        <!-- xyz = 0, -13mm / 2, 29mm -->
        <origin rpy="0 0 0" xyz="0 -0.0065 -0.029"/>

        <!-- Lower, Upper: +/- 60 deg. Velocity, effort are arbitrary -->
        <limit effort="30" velocity="1.0" lower="-1.05" upper="1.05"/>
	</joint>

    <joint name="base_to_left_leg" type="revolute">
		<parent link="base_link"/>
		<child link="left_leg"/>
		<axis xyz="0 -1 0"/>

        <!-- xyz = 0, 13mm / 2, 29mm -->
        <origin rpy="0 0 0" xyz="0 0.0065 -0.029"/>

        <!-- Lower, Upper: +/- 60 deg. Velocity, effort are arbitrary -->
        <limit effort="30" velocity="1.0" lower="-1.05" upper="1.05"/>
	</joint>

	<joint name="base_to_roll_rw" type="continuous">
		<parent link="base_link"/>
		<child link="roll_rw"/>
		<axis xyz="0 0 1"/>
        
        <!-- rpy = 0, pi/2, 0 -->
        <!-- TODO: Update xyz from Matt -->
		<origin rpy="0 1.57075 0" xyz="0 -0.035 0.0375"/>
	</joint>

    <joint name="base_to_pitch_rw" type="continuous">
		<parent link="base_link"/>
		<child link="pitch_rw"/>
		<axis xyz="0 0 1"/>
        
        <!-- rpy = 0, 0, 0 -->
        <!-- TODO: Update xyz from Matt -->
		<origin rpy="0 0 0" xyz="0 0.02 0.0375"/>
	</joint>

    <joint name="base_to_yaw_rw" type="continuous">
		<parent link="base_link"/>
		<child link="yaw_rw"/>
		<axis xyz="0 0 1"/>
        
        <!-- rpy = pi/2, 0, 0 -->
        <!-- TODO: Update xyz from Matt -->
		<origin rpy="1.57075 0 0" xyz="0 0.055 0.0375"/>
	</joint>

</robot>